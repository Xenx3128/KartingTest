@page
@model TestMVC.Pages.AccModel

@{
    Layout = "_AccountLayout";
}


<div class="content-panel">
    <h4 class="red-heading">История заездов</h4>
    <table id="raceHistory" class="cell-border stripe">
        <thead>
            <tr>
                <th>Дата заезда</th>
                <th>Время заезда</th>
                <th>Категория</th>
                <th>Карт</th>
                <th>Место</th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <th>01.01.2000</th>
                <th>8:15-8:30</th>
                <th>Детский</th>
                <th>Двухместный</th>
                <th>1</th>
            </tr>
            <tr>
                <th>04.02.2000</th>
                <th>9:15-9:30</th>
                <th>Взрослый</th>
                <th>Одноместный</th>
                <th>2</th>
            </tr>
            <tr>
                <th>05.03.2000</th>
                <th>10:15-10:30</th>
                <th>Детский</th>
                <th>Одноместный</th>
                <th>3</th>
            </tr>
        </tbody>
    </table>

</div>



<link rel="stylesheet" href="~/css/account.css">




@section Scripts{
    <script type="module" src='@Url.Content("~/js/history.js")'></script>
    <script>
        var menu_link = document.getElementById("acc-menu-link-2");
        menu_link.classList.add("active");
    </script>
}
